{
  "env": {
    "port": 3444
  },
  "routes": {
    "/person": {
      "method": "GET",
      "flow": {
        "Set value request header": {
          "pluginName":"dro-set-req-header",
          "header": {
            "test_header": "value"
          }
        },
        "Set value2 request header": {
          "pluginName":"dro-set-req-header",
          "header": {
            "test_header_2": "value2"
          }
        },
        "Set value response header": {
          "pluginName":"set_response_header",
          "isLocalPlugin":true,
          "header": {
            "test_header_rsp": "valueResponse"
          }
        },
        "call another dro file": {
          "isLocalPlugin":true,
          "pluginName":"composite",
          "path":"../test/integration/dromedaryFile2.json"
        }
      }
    }
  }
}
